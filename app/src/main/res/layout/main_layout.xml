<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:scrollbars="none"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!-- Type / Setting -->
        <RelativeLayout
            android:id="@+id/FrameType"
            android:layout_width="match_parent"
            android:layout_height="@dimen/frame_height"
            android:layout_marginBottom="@dimen/frame_bottom_margin"
            android:layout_marginEnd="@dimen/frame_right_margin"
            android:layout_marginStart="@dimen/frame_left_margin"
            android:layout_marginTop="10dip"
            android:background="@drawable/bg_areas"
            android:paddingBottom="@dimen/frame_bottom_padding"
            android:paddingEnd="@dimen/frame_right_padding"
            android:paddingStart="@dimen/frame_left_padding"
            android:paddingTop="@dimen/frame_top_padding">

            <TextView
                android:id="@+id/hl_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableRight="@drawable/arrow_right"
                android:text="@string/lb_contacttype"
                android:textAllCaps="true"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/info_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:text="@string/no_selection"
                android:textSize="13sp" />

            <HorizontalScrollView
                android:id="@+id/hsv_type"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                android:measureAllChildren="false"
                android:scrollbars="none">

                <RelativeLayout
                    android:id="@+id/innerlayer_type"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:paddingTop="25dip">

                    <ToggleButton
                        android:id="@+id/btn_type_1"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_shopping"
                        android:contentDescription="@string/btn_type_1"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_2"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_1"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_1"
                        android:background="@drawable/btn_home"
                        android:contentDescription="@string/btn_type_2"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_3"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_2"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_2"
                        android:background="@drawable/btn_food"
                        android:contentDescription="@string/btn_type_3"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_4"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_3"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_3"
                        android:background="@drawable/btn_leisure"
                        android:contentDescription="@string/btn_type_4"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_5"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_4"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_4"
                        android:background="@drawable/btn_work"
                        android:contentDescription="@string/btn_type_5"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_6"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_5"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_5"
                        android:background="@drawable/btn_travel"
                        android:contentDescription="@string/btn_type_6"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_7"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_6"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_6"
                        android:background="@drawable/btn_prayer"
                        android:contentDescription="@string/btn_type_7"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_8"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_7"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_7"
                        android:background="@drawable/btn_online"
                        android:contentDescription="@string/btn_type_8"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_type_9"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_type_8"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_type_8"
                        android:background="@drawable/btn_question_type"
                        android:contentDescription="@string/btn_type_9"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />
                </RelativeLayout>
            </HorizontalScrollView>
        </RelativeLayout>

        <!-- Relationship -->
        <RelativeLayout
            android:id="@+id/FrameRelationship"
            android:layout_width="match_parent"
            android:layout_height="@dimen/frame_height"
            android:layout_below="@id/FrameType"
            android:layout_marginBottom="@dimen/frame_bottom_margin"
            android:layout_marginEnd="@dimen/frame_right_margin"
            android:layout_marginStart="@dimen/frame_left_margin"
            android:layout_marginTop="@dimen/frame_top_margin"
            android:background="@drawable/bg_areas"
            android:paddingBottom="@dimen/frame_bottom_padding"
            android:paddingEnd="@dimen/frame_right_padding"
            android:paddingStart="@dimen/frame_left_padding"
            android:paddingTop="@dimen/frame_top_padding">

            <TextView
                android:id="@+id/hl_rel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableRight="@drawable/arrow_right"
                android:text="@string/lb_relationship"
                android:textAllCaps="true"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/info_rel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:text="@string/no_selection"
                android:textSize="13sp" />

            <HorizontalScrollView
                android:id="@+id/hsv_rel"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                android:measureAllChildren="false"
                android:scrollbars="none">

                <RelativeLayout
                    android:id="@+id/innerlayer_rel"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingTop="25dip">

                    <ToggleButton
                        android:id="@+id/btn_relate_1"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_friend"
                        android:contentDescription="@string/btn_relate_1"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_2"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_1"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_1"
                        android:background="@drawable/btn_acquaint"
                        android:contentDescription="@string/btn_relate_2"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_3"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_2"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_2"
                        android:background="@drawable/btn_colleague"
                        android:contentDescription="@string/btn_relate_3"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_4"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_3"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_3"
                        android:background="@drawable/btn_customer"
                        android:contentDescription="@string/btn_relate_4"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_5"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_4"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_4"
                        android:background="@drawable/btn_stranger"
                        android:contentDescription="@string/btn_relate_5"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_6"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_5"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_5"
                        android:background="@drawable/btn_clerk"
                        android:contentDescription="@string/btn_relate_6"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_7"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_6"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_6"
                        android:background="@drawable/btn_neighbour"
                        android:contentDescription="@string/btn_relate_7"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_8"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_7"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_7"
                        android:background="@drawable/btn_partner"
                        android:contentDescription="@string/btn_relate_8"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_9"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_8"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_8"
                        android:background="@drawable/btn_relative"
                        android:contentDescription="@string/btn_relate_9"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                    <ToggleButton
                        android:id="@+id/btn_relate_10"
                        style="@style/toggleButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/btn_relate_9"
                        android:layout_marginLeft="7dip"
                        android:layout_toRightOf="@+id/btn_relate_9"
                        android:background="@drawable/btn_question_rel"
                        android:contentDescription="@string/btn_relate_10"
                        android:minHeight="@dimen/btn_height"
                        android:minWidth="@dimen/btn_width" />

                </RelativeLayout>
            </HorizontalScrollView>
        </RelativeLayout>

        <!-- Gender / Age -->
        <RelativeLayout
            android:id="@+id/FrameGenAge"
            android:layout_width="match_parent"
            android:layout_height="@dimen/frame_height"
            android:layout_below="@id/FrameRelationship"
            android:layout_marginBottom="@dimen/frame_bottom_margin"
            android:layout_marginEnd="@dimen/frame_right_margin"
            android:layout_marginStart="@dimen/frame_left_margin"
            android:layout_marginTop="@dimen/frame_top_margin"
            android:background="@drawable/bg_areas"
            android:paddingBottom="@dimen/frame_bottom_padding"
            android:paddingEnd="@dimen/frame_right_padding"
            android:paddingStart="@dimen/frame_left_padding"
            android:paddingTop="@dimen/frame_top_padding">

            <TextView
                android:id="@+id/hl_genage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lb_genage"
                android:textAllCaps="true"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/info_gen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:text="@string/no_selection"
                android:textSize="13sp" />

            <RelativeLayout
                android:id="@+id/innerlayer_genage"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:paddingTop="25dip">

                <ToggleButton
                    android:id="@+id/btn_gen_1"
                    style="@style/toggleButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_female"
                    android:contentDescription="@string/btn_female"
                    android:minHeight="@dimen/btn_height"
                    android:minWidth="@dimen/btn_width" />

                <ToggleButton
                    android:id="@+id/btn_gen_2"
                    style="@style/toggleButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/btn_gen_1"
                    android:layout_marginLeft="7dip"
                    android:layout_toRightOf="@+id/btn_gen_1"
                    android:background="@drawable/btn_male"
                    android:contentDescription="@string/btn_male"
                    android:minHeight="@dimen/btn_height"
                    android:minWidth="@dimen/btn_width" />

                <ToggleButton
                    android:id="@+id/btn_gen_3"
                    style="@style/toggleButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/btn_gen_2"
                    android:layout_marginLeft="7dip"
                    android:layout_toRightOf="@+id/btn_gen_2"
                    android:background="@drawable/btn_question_gen"
                    android:contentDescription="@string/btn_genother"
                    android:minHeight="@dimen/btn_height"
                    android:minWidth="@dimen/btn_width" />

                <RelativeLayout
                    android:id="@+id/ageFrame"
                    style="?android:attr/buttonStyle"
                    android:layout_width="120dip"
                    android:layout_height="@dimen/btn_height"
                    android:padding="5dip"
                    android:layout_marginLeft="7dip"
                    android:layout_alignTop="@+id/btn_gen_3"
                    android:layout_toRightOf="@+id/btn_gen_3"
                    android:background="@drawable/bg_edittext">

                    <TextView
                        android:id="@+id/lb_age"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dip"
                        android:layout_alignParentTop="true"
                        android:text="@string/lb_age"
                        android:textSize="12sp"
                        android:textColor="@color/btn_icon_genage_off"/>

                    <EditText
                        android:id="@+id/age_input"
                        android:hint=""
                        android:layout_width="78dip"
                        android:layout_height="25dip"
                        android:ems="5"
                        android:padding="2dip"
                        android:cursorVisible="false"
                        android:gravity="center_horizontal"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLength="2"
                        android:singleLine="true"
                        android:layout_alignParentBottom="true"
                        android:textColor="@color/btn_icon_genage_off"
                        android:background="@drawable/bg_white" />

                    <Button android:id="@+id/btn_x_close"
                        android:layout_width="25dip"
                        android:layout_height="25dip"
                        android:layout_marginLeft="5dip"
                        android:background="@drawable/btn_clearage"
                        android:onClick="clearAge"
                        android:layout_alignTop="@+id/age_input"
                        android:layout_toRightOf="@+id/age_input" />

                </RelativeLayout>

            </RelativeLayout>
        </RelativeLayout>

        <!-- Approx. Duration -->
        <RelativeLayout
            android:id="@+id/FrameDuration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/FrameGenAge"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/frame_bottom_margin"
            android:layout_marginEnd="@dimen/frame_right_margin"
            android:layout_marginStart="@dimen/frame_left_margin"
            android:layout_marginTop="@dimen/frame_top_margin"
            android:background="@drawable/bg_areas"
            android:minHeight="@dimen/frame_height_small"
            android:paddingBottom="@dimen/frame_bottom_padding"
            android:paddingEnd="15dip"
            android:paddingStart="@dimen/frame_left_padding"
            android:paddingTop="@dimen/frame_top_padding"
            tools:ignore="MissingPrefix">

            <TextView
                android:id="@+id/hl_dur"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lb_duration"
                android:textAllCaps="true"
                android:textSize="13sp" />

            <!-- Hour -->
            <RelativeLayout
                android:id="@+id/box_hour"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dip"
                android:background="@drawable/bg_on"
                android:minHeight="72dip"
                android:minWidth="52dip">

                <TextView
                    android:id="@+id/txt_hour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="@string/hours"
                    android:layout_marginTop="4dip"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/duration_hour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_hour"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dip"
                    android:textSize="48sp"
                    fontPath="fonts/digitalism.ttf" />
                </RelativeLayout>

                <Button
                    android:id="@+id/hour_plus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="6dip"
                    android:layout_marginEnd="6dip"
                    android:layout_marginStart="6dip"
                    android:layout_marginTop="25dip"
                    android:layout_toRightOf="@id/box_hour"
                    android:background="@drawable/btn_plus"
                    android:minHeight="28dip"
                    android:minWidth="43dip" />

                <Button
                    android:id="@+id/hour_minus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/hour_plus"
                    android:layout_marginEnd="6dip"
                    android:layout_marginStart="6dip"
                    android:layout_toRightOf="@id/box_hour"
                    android:background="@drawable/btn_minus"
                    android:minHeight="28dip"
                    android:minWidth="43dip" />

            <!-- Minutes -->
            <RelativeLayout
                android:id="@+id/box_min"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dip"
                android:layout_toRightOf="@id/hour_plus"
                android:background="@drawable/bg_on"
                android:minHeight="72dip"
                android:minWidth="52dip">

                <TextView
                    android:id="@+id/txt_min"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="@string/minutes"
                    android:layout_marginTop="4dip"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/duration_min"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_min"
                    android:layout_marginTop="5dip"
                    fontPath="fonts/digitalism.ttf"
                    android:layout_centerHorizontal="true"
                    android:textSize="48sp" />
            </RelativeLayout>

            <Button
                android:id="@+id/min_plus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dip"
                android:layout_marginBottom="7dip"
                android:layout_toRightOf="@id/box_min"
                android:background="@drawable/btn_plus"
                android:layout_marginStart="6dip"
                android:layout_marginEnd="6dip"
                android:minHeight="28dip"
                android:minWidth="42dip" />

            <Button
                android:id="@+id/min_minus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/box_min"
                android:layout_below="@id/min_plus"
                android:background="@drawable/btn_minus"
                android:layout_marginStart="6dip"
                android:layout_marginEnd="6dip"
                android:minHeight="28dip"
                android:minWidth="42dip" />

            <!-- Second -->
            <RelativeLayout
                android:id="@+id/box_sec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dip"
                android:layout_toRightOf="@id/min_plus"
                android:minHeight="72dip"
                android:minWidth="52dip"
                android:background="@drawable/bg_on">

                <TextView
                    android:id="@+id/txt_sec"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="@string/seconds"
                    android:layout_marginTop="4dip"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/duration_sec"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_sec"
                    fontPath="fonts/digitalism.ttf"
                    android:layout_marginTop="5dip"
                    android:layout_centerHorizontal="true"
                    android:textSize="48sp" />
            </RelativeLayout>

            <Button
                android:id="@+id/sec_plus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dip"
                android:layout_toRightOf="@id/box_sec"
                android:layout_marginStart="6dip"
                android:layout_marginBottom="7dip"
                android:background="@drawable/btn_plus"
                android:minHeight="28dip"
                android:minWidth="42dip" />

            <Button
                android:id="@+id/sec_minus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/box_sec"
                android:layout_below="@id/sec_plus"
                android:layout_marginStart="6dip"
                android:background="@drawable/btn_minus"
                android:minHeight="28dip"
                android:minWidth="42dip" />

        </RelativeLayout>

        <!-- Group Typicality new -->
        <RelativeLayout
            android:id="@+id/FrameTypicality"
            android:layout_width="match_parent"
            android:layout_height="@dimen/frame_height"
            android:layout_below="@id/FrameDuration"
            android:layout_marginBottom="@dimen/frame_bottom_margin"
            android:layout_marginEnd="@dimen/frame_right_margin"
            android:layout_marginStart="@dimen/frame_left_margin"
            android:layout_marginTop="@dimen/frame_top_margin"
            android:background="@drawable/bg_areas"
            android:paddingBottom="@dimen/frame_bottom_padding"
            android:paddingEnd="@dimen/frame_right_padding"
            android:paddingStart="@dimen/frame_left_padding"
            android:paddingTop="@dimen/frame_top_padding">

            <TextView
                android:id="@+id/hl_typical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lb_typical"
                android:textAllCaps="true"
                android:textSize="13sp" />

